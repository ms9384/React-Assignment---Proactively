{
  "expo": {
    "extra": {
      "API_BASE_URL": "http://192.168.1.4:8000"
    },
    "name": "proactively",
    "slug": "proactively",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "../assets/images/icon.png",
    "scheme": "proactively",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#204CBB"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "../assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
